    </servlet>

    <servlet-mapping>
        <servlet-name>coverage.web.DFGraphCoverage1</servlet-name>
        <url-pattern>/coverage/DFGraphCoverage1</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>coverage.web.MinimalMUMCUTCoverage</servlet-name>
        <servlet-class>coverage.web.MinimalMUMCUTCoverage</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>coverage.web.MinimalMUMCUTCoverage</servlet-name>
        <url-pattern>/coverage/MinimalMUMCUTCoverage</url-pattern>
    </servlet-mapping>
    
