<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
	<display-name>
	CoverageWebApplication</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	 <!--  servlet mappings start -->

    <servlet>
        <servlet-name>coverage.web.GraphCoverage</servlet-name>
        <servlet-class>coverage.web.GraphCoverage</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>coverage.web.GraphCoverage</servlet-name>
        <url-pattern>/coverage/GraphCoverage</url-pattern>
    </servlet-mapping>
    
  <servlet>
        <servlet-name>coverage.web.LogicCoverage</servlet-name>
        <servlet-class>coverage.web.LogicCoverage</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>coverage.web.LogicCoverage</servlet-name>
        <url-pattern>/coverage/LogicCoverage</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>coverage.web.DFGraphCoverage</servlet-name>
        <servlet-class>coverage.web.DFGraphCoverage</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>coverage.web.DFGraphCoverage</servlet-name>
        <url-pattern>/coverage/DFGraphCoverage</url-pattern>
    </servlet-mapping>

	<servlet>
        <servlet-name>coverage.web.DFGraphCoverage1</servlet-name>
